                        </Grid>
                </Border>
                <Border Grid.Column="2" Grid.Row="2" BorderBrush="Black" BorderThickness="1">
                        <Grid ColumnDefinitions="*,*,*" RowDefinitions="*,*,*">
                                <Border Grid.Column="0" Grid.Row="0" Background="White" BorderBrush="DarkGray" BorderThickness="1"/>
                                <Border Grid.Column="1" Grid.Row="0" Background="White" BorderBrush="DarkGray" BorderThickness="1"/>
